{
  "name": "@j.su/nota",
  "version": "0.0.0",
  "description": "note-taking app",
  "main": "use/scripts/nota.js",
  "scripts": {
    "watchify": "watchify -vd -e src/main.js -o use/scripts/nota.js",
    "serve": "lite-server",
    "dev": "npm-run-all --parallel watchify serve",
    "build": "cross-env NODE_ENV=production browserify -g envify src/main.js | uglifyjs -c warnings=false -m > use/scripts/nota.js",
    "lint": "eslint --ext .js,.vue src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SundarJ/nota.git"
  },
  "author": "Sundar Joshi",
  "license": "MIT",
  "dependencies": {
    "history": "^3.0.0",
    "path-to-regexp": "^1.5.3",
    "rangy": "^1.3.0",
    "setimmediate": "^1.0.4",
    "vue": "^2.0.0-beta.1",
    "wysiwyg.js": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "6.6.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^1.0.7",
    "envify": "^3.4.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.0",
    "lite-server": "^2.2.0",
    "npm-run-all": "^2.0.0",
    "stylus": "^0.54.5",
    "uglify-js": "^2.6.2",
    "vueify": "^9.2.3",
    "watchify": "^3.7.0"
  }
}
